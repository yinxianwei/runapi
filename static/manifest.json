{
    "manifest_version": 2,
    "name": "runapi",
    "version": "1.0",
    "description": "runapi",
    "browser_action": {
        "default_title": "runapi",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "http://*/"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
        {
            "matches": [
                "http://showdoc.mei1.info/*"
            ],
            "css": [],
            "js": [
                "myscript.js"
            ]
        }
    ]
}